// Maintenir synchro avec UICommonCheckBox.h
CheckBoxFullState is TBaseClass
(
    // bits 0-1 : état décoché/coché/indeterminé
    UNCHECKED       is 0x0
    CHECKED         is 0x1
    INDETERMINATE   is 0x2
    // bit 2 : état inactif/actif
    INACTIVE        is 0x0
    ACTIVE          is 0x4
    // bits 3-4 : état sans pointeur/survolé/pressé
    UNTOUCHED       is 0x0
    HOVERED         is 0x8
    PRESSED         is 0x10
)

