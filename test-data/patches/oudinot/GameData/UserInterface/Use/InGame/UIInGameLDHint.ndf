                            )

                            TextureToken = <TextureSteelman>
                            TextureFrame = TUIFramePropertyRTTI
                            (
                                RelativeWidthHeight = [1.0, 1.0]
                            )
                        )
            ] : [] )
            +
            (<TextureSteelman_FullScreen> != "" ?
            [
                BUCKTextureDescriptor
